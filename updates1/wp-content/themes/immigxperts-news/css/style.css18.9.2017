/* tables */
table.tablesorter {
	font-family:arial;
	background-color: #CDCDCD;
	margin:10px 0pt 15px;
	font-size: 8pt;
	width: 100%;
	text-align: left;
}
table.tablesorter thead tr th, table.tablesorter tfoot tr th {
	background-color: #007AAD;
	border-right: 1px solid #FFF;
	font-size: 10pt;
	color:#fff;
	font-weight: normal;
}
table.tablesorter thead tr .header {
	background-image: url(bg.gif);
	background-repeat: no-repeat;
	background-position: center right;
	cursor: pointer;
}
table.tablesorter tbody td {
	color: #3D3D3D;
    font-size: 10pt;
	vertical-align: top;
	border-bottom: 1px solid #fff;
}
table.tablesorter tbody tr.odd td {
	background-color:#F0F0F6;
}
table.tablesorter thead tr .headerSortUp {
	background-image: url(asc.gif);
}
table.tablesorter thead tr .headerSortDown {
	background-image: url(desc.gif);
}
table.tablesorter thead tr .headerSortDown, table.tablesorter thead tr .headerSortUp {
background-color: #007AAD;
}

table.tablesorter tr:nth-of-type(2n){background-color:#e3e3e3;}

table.tablesorter tr:nth-of-type(2n+1){background-color:#fff;}
.tablesorter td.green {
    background: #63C206 none repeat scroll 0 0;
}

.tablesorter td.red {
    background: #DD0000 none repeat scroll 0 0;
    color: white;
}

.tablesorter td.orange {
    background: #faaa31 none repeat scroll 0 0;
}
.tablesorter td.high_sa {
    background: #00a887 none repeat scroll 0 0;
}
.tablesorter td.low_sa {
    background: #5bc0de none repeat scroll 0 0;
}
.tablesorter td.medium_sa {
    background: #f0ad4e none repeat scroll 0 0;
}
.tablesorter td.special_sa {
    background: #DD0000 none repeat scroll 0 0;
    color: white;
}
.tablesorter td.green_dark {
    background: #40b75a none repeat scroll 0 0;
}

